<mvc:View controllerName="mysapui5_sandbox_app.controller.MyView" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<content>
					<Button text="Authorize M$ Graph API" press="onAuthorize"/>
					<Button text="Logout M$ Graph API" press="onLogOut"/>
					<Button text="Read My Recent Files" press="onGetMyRecentFiles"/>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>